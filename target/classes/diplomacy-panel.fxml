<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="viewControllers.Info.DiplomacyPanel"
            prefHeight="400.0" prefWidth="600.0">

    <center>
        <VBox alignment="CENTER" spacing="5">
            <Label text="diplomacy menu" prefWidth="300" textFill="crimson" style="-fx-font-size: 20;" alignment="CENTER"/>
            <Button fx:id="back" text="back" prefWidth="200" onMouseClicked="#back"/>
            <Button fx:id="trade" text="trading" prefWidth="200" onMouseClicked="#tradePanel"/>
            <Button fx:id="discuss" text="discuss" prefWidth="200" onMouseClicked="#discussPanel"/>
        </VBox>
    </center>
</BorderPane>
