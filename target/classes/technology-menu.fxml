<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="viewControllers.Info.TechnologyMenu"
            prefHeight="400.0" prefWidth="600.0">

    <center>
        <ScrollPane fitToWidth="true" fitToHeight="true">
            <VBox alignment="CENTER" spacing="30">
                <Button fx:id="technologyTree" text="technology tree" alignment="CENTER" onMouseClicked="#technologyTree"/>
                <VBox fx:id="technologyPlayerHave" alignment="CENTER"/>
                <VBox fx:id="technologyObtained" alignment="CENTER"/>
            </VBox>
        </ScrollPane>
    </center>
</BorderPane>
